{
  "name": "bookingapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "e2e:build-ios": "detox build --configuration ios",
    "e2e:build-android": "detox build --configuration android",
    "e2e:run-ios": "detox test --configuration ios",
    "e2e:run-android": "detox test --configuration android"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@use-expo/font": "^2.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "expo-app-loading": "^2.0.0",
    "expo-font": "^10.1.0",
    "galio-framework": "^0.8.0",
    "node-sass": "^7.0.1",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-sass-transformer": "^2.0.0",
    "react-native-screens": "^3.13.1",
    "twrnc": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.18.3",
    "@cucumber/cucumber": "^8.3.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "detox": "^19.7.1",
    "eslint": "^7.32.0",
    "eslint-plugin-detox": "^1.0.0",
    "expo": "~45.0.0",
    "expo-status-bar": "~1.3.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.release": {
        "binaryPath": "./ios/build/Build/Products/Release-iphonesimulator/bookingapp.app",
        "build": "xcodebuild -workspace ios/bookingapp.xcworkspace -configuration release -scheme bookingapp -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  }
}
