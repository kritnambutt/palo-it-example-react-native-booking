{
  "name": "bookingapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "e2e:test": "detox test -c ios.release",
    "e2e:build": "detox build -c ios.release"
